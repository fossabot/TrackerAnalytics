{
  "manifest_version": 2,
  "name": "Tracker Analytics",
  "version": "0.1.0",
  "author": "Emin Muhammadi",
  "description": "A socket based app for TrackerAnalytics",
  "permissions": [
    "background",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./lib/client.min.js",
        "./lib/crypto.min.js",
        "./lib/socket.io.min.js",
        "./lib/rrweb.js",
        "./src/main.js",
        "content.js"
      ]
    }
  ]
}
