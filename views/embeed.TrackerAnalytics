<!doctype html>
<html lang="en" dir="auto">
<head>
    <base href="#homeUrl#"/>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <link rel="stylesheet" href="/assets/rrweb.css"/>
    <script src="/assets/rrweb.min.js"></script>
</head>
<body>
      <script type="module">
          fetch(`#url#`)
              .then(response => response.json())
              .then((data) => {
                  const events = [];
                  for (let i=0; i<data.data.length; i++)
                  {
                      events.push(JSON.parse(data.data[i].eventDataJSON));
                  }
                  const webPlayer = window.rrweb;
                  const getVideo  = new webPlayer.Replayer(events.reverse());
                  getVideo.play();
          });
      </script>
</body>
</html>