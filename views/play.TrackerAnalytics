<body oncontextmenu="return false;" style="cursor:none">
      <script type="module">
        let   events    = [];
        const sessions  = JSON.parse(unescape("{{%sessions%}}"));

        for(let i=0; i<(sessions.data).length; i++)
        {
             events.push(JSON.parse(sessions.data[i].eventDataJSON));
        }

        const webPlayer = window.rrweb;
        const getVideo  = new webPlayer.Replayer(events.reverse());
        getVideo.play();
      </script>
